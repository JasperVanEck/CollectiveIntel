    "def mean_center_columns(matrix):\n",
    "    result = matrix.copy()\n",
    "    \n",
    "    for i in result.columns.values:\n",
    "        m = result[i].mean()\n",
    "        for j in result.index.values:\n",
    "            result[i][j] = result[i][j] - m\n",
    "            \n",
    "    return result\n",
