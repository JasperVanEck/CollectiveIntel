    "# TEST\n",
    "def weighted_mean(neighborhood, ratings):  \n",
    "    upper = 0\n",
    "    bottom = 0\n",
    "    \n",
    "    for index in neighborhood.index:\n",
    "        similarity = neighborhood.get(index)\n",
    "        upper += ratings.get(index) * similarity\n",
    "        bottom += similarity\n",
    "    \n",
    "    if bottom == 0:\n",
    "        return np.nan\n",
    "    else:\n",
    "        return upper / bottom\n",
